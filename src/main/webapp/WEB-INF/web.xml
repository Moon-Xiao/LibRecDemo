<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >
<web-app>
  <display-name>Archetype Created Web Application</display-name>
    <welcome-file-list>
        <welcome-file>index</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>IndexServlet</servlet-name>
        <servlet-class>neu.moon.servlet.IndexServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetSplitConfServlet</servlet-name>
        <servlet-class>neu.moon.servlet.GetSplitConfServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetSimilarityConfServlet</servlet-name>
        <servlet-class>neu.moon.servlet.GetSimilarityConfServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetRecommendConfServlet</servlet-name>
        <servlet-class>neu.moon.servlet.GetRecommendConfServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetRecommendConfServlet</servlet-name>
        <url-pattern>/getRecommendConf.ajax</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetSimilarityConfServlet</servlet-name>
        <url-pattern>/getSimilarityConf.ajax</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetSplitConfServlet</servlet-name>
        <url-pattern>/getSplitConf.ajax</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>IndexServlet</servlet-name>
        <url-pattern>/index</url-pattern>
    </servlet-mapping>
</web-app>
